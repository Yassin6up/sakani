{
  "expo": {
    "name": "سكني جو - Sakani jo",
    "slug": "sakaniJo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBktXZcfSRHQyWCBfHhedaH3fPTBstxU64"
      } ,
       "bundleIdentifier": "com.sakaniJo.sakaniJo" ,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need access to your location to show nearby listings.",
        "NSLocationAlwaysUsageDescription": "We need access to your location to show nearby listings."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAjyJoeDP5a6ZJq8ggBcIOLjGGzPLzLxog"
        }
      },
      "package": "com.sakaniJo.sakaniJo",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SakaniJo to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b32100bf-bf64-4069-82ca-4271da782e35"
      }
    },
    "owner": "yassin6up"
  },
  "android": {
    "enableHermes": false
  },
  "ios": {
    "enableHermes": false
  }
}
